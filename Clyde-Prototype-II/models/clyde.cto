
namespace Clyde

concept SensorData {
  o String SensorName
  o Double Latitude
  o Double Longitude
  o Double TimeStamp
  o Double Temperature
}

asset SensorReadings identified by ReadingId {
  o String       ReadingId
  o SensorData[] Readings
}

transaction Initialise

transaction TemperatureReading {
  o SensorData SD
  --> SensorReadings SR
}
